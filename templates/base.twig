{% block html_head_container %}

	{% include 'html-header.twig' %}
	{% block head %}{% endblock %}
</head>{% endblock %}<body class="{{body_class}}" data-template="base.twig">
<header class="header main-header">
	{% block header %}
		<div class="header_wrap">
			<div class="btn-menu">
				<button class="menu" type="button"><span></span><span></span><span></span></button>
				<button class="menu_m" type="button"><span></span><span></span><span></span></button>
			</div>
			<div class="header_logo">
				<a href="{{ site.link }}" class="logo">
					<img class="logo-d" src="{{ theme.link }}/static/img/logo_d.png" alt="{{ site.name }}">
					<img class="logo-w" src="{{ theme.link }}/static/img/logo_w.png" alt="{{ site.name }}">
				</a>
				<p class="quotes">
					{% set quote = random( quotes ) %}
					{{ quote.text }}
					-
					{{ quote.source }}
				</p>
			</div>
			<div class="contact-block">
				<ul class="list">
					<li class="item">
						<a href="#" class="contact-link">
							<i class="icon icon-telegram"></i>
						</a>
					</li>
					<li class="item">
						<a href="#" class="contact-link">
							<i class="icon icon-fb"></i>
						</a>
					</li>
					<li class="item">
						<a href="#" class="contact-link">
							<i class="icon icon-gab"></i>
						</a>
					</li>
					<li class="item">
						<a href="#" class="contact-link">
							<i class="icon icon-tw"></i>
						</a>
					</li>
					<li class="item">
						<a href="#" class="contact-link">
							<i class="icon icon-rss"></i>
						</a>
					</li>
				</ul>
			</div>
			<div class="search-block">
				<button class="btn-search" type="button">
					<i class="icon icon-search"></i>
				</button>
			</div>
			<div class="search-row">
				<form action="{{ site.url}}" method="get" class="search-form">
					<input type="text" class="input-search" placeholder="Search" name="s"/>
					<div class="btn-block">
						<button class="btn-search" type="submit">
							<i class="icon icon-search"></i>
						</button>
					</div>
				</form>
			</div>
		</div>
		{% include 'partial/main-menu.twig' %}
	{% endblock %}
</header>

<main class="main">
	{% block main %}
	<div class="container flex-container">
		<div class="main-wrapper">
			{% block title %}
				{% if title %}
					<h1 class="page-title">{{title}}</h1>
				{% endif %}
			{% endblock %}
			{% block content %}
				Sorry, no content
			{% endblock %}
		</div>
		<aside class="sidebar">
			<div class="sidebar-widgets">
				{% include 'sidebar.twig' %}
			</div>
		</aside>
	</div>
	{% endblock %}
</main>

{% block footer %}
	<footer class="footer">
			{% include 'footer.twig' %}
	</footer>
	{{ function('wp_footer') }}
{% endblock %}</body></html>
